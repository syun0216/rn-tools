{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": ["plugin:prettier/recommended"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "no-duplicate-imports": 2
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"]
  }
  